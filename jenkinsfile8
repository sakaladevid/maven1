pipeline
{
    agent any
     stages
     {
      stage('continousDownload_loans')
      {
           steps
           {
               git 'https://github.com/sakaladevid/maven1.git'
            }
        }
        stage('continousBuild_loans')
        {
            steps
            {
                sh 'mvn package'
             }
         }
         
         
             
             
                
             
         
         
         
        
             
                
                
           
           
       
  
   
    
    
        
        
            
        
       
        

        

